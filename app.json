{
  "expo": {
    "name": "photos-backup",
    "slug": "photos-backup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Formula1-Black.ttf", "./assets/fonts/Formula1-Bold_web.ttf", "./assets/fonts/Formula1-Bold-4.ttf", "./assets/fonts/Formula1-Italic.ttf", "./assets/fonts/Formula1-Regular.ttf", "./assets/fonts/Formula1-Wide.ttf",
        "./assets/fonts/Montserrat-Regular.ttf"]
        }
      ]
    ]
  }
}
